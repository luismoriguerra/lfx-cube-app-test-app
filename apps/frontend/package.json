{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/setenv.ts",
    "start": "yarn run config && ng serve --configuration=local",
    "start:remote": "yarn run config && ng serve --configuration=development --host 0.0.0.0",
    "start:dev": "yarn run config && ng serve --configuration=development",
    "start:prs": "yarn run config && ng serve --configuration=prs",
    "build:dev": "yarn run config && ng build --configuration=development",
    "build:prod": "yarn run config && ng build",
    "build": "yarn run config && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "bash scripts/e2e.sh -m h",
    "test-open": "bash scripts/e2e.sh",
    "lint": "pretty-quick --staged && ng lint --fix",
    "postinstall": "yarn pretty-quick",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome --headless --config video=false,screenshotOnRunFailure=false,watchForFileChanges=false",
    "deploy": "serverless deploy",
    "deploy:client": "serverless client deploy",
    "deploy:feature": "serverless deploy && serverless syncToS3 && sls domainInfo",
    "serverless:remove": "serverless remove",
    "serverless:remove:client": "serverless client remove",
    "clean": "rm -rf dist .angular .turbo dist node_modules"
  },
